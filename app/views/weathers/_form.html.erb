<%= simple_form_for(@weather) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :temp %>
    <%= f.input :descr %>
    <%= f.input :wind %>
    <%= f.input :wind_direction %>
    <%= f.input :barp %>
    <%= f.input :hum %>
    <%= f.input :created_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
